{
	"name" : "alsciende/netrunnerdb",
	"description" : "NetrunnerDB",
	"type" : "project",
	"license" : "MIT",
	"require" : {
		"symfony/symfony" : "3.2.*",
		"friendsofsymfony/oauth-server-bundle" : "^1.4.2",
		"gedmo/doctrine-extensions" : "2.3.*",
		"symfony/swiftmailer-bundle" : "~2.3",
		"friendsofsymfony/user-bundle" : "2.0.*",
		"doctrine/orm" : "~2.2,>=2.2.3",
		"incenteev/composer-parameter-handler" : "~2.0",
		"friendsofsymfony/jsrouting-bundle" : "^2.0",
		"twig/extensions" : "~1.0",
		"michelf/php-markdown" : "~1.4",
		"php" : ">=5.3.3",
		"sensio/generator-bundle" : "^3.1",
		"symfony/monolog-bundle" : "~2.4",
		"sensio/framework-extra-bundle" : "~3.0",
		"sensio/distribution-bundle" : "^5.0",
		"doctrine/doctrine-bundle" : "~1.2",
		"ezyang/htmlpurifier": "~4.6",
		"kriswallsmith/assetic": "^1.3",
		"nelmio/api-doc-bundle": "^2.12",
		"willdurand/jsonp-callback-validator": "^1.1",
		"jms/serializer": "^1.1",
		"friendsofsymfony/rest-bundle": "^1.7",
		"jms/serializer-bundle": "^1.1",
		"guzzlehttp/guzzle": "^6.2",
		"twig/twig": "^1.0",
		"symfony/framework-bundle": "~3.0",
		"friendsofsymfony/oauth2-php": "^1.2",
		"symfony/assetic-bundle": "^2.8",
		"stof/doctrine-extensions-bundle": "^1.2"
	},
	"require-dev" : {
	},
	"autoload" : {
		"psr-0" : {
			"" : "src/"
		}
	},
	"minimum-stability" : "stable",
	"config" : {
		"bin-dir" : "bin"
	},
	"scripts" : {
		"post-install-cmd" : [
			"Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
			"Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
			"Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
			"Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
			"Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
		],
		"post-update-cmd" : [
			"Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
			"Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
			"Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
			"Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
			"Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
		]
	},
	"extra" : {
		"branch-alias" : {
			"dev-master" : "2.3-dev"
		},
		"symfony-web-dir" : "web",
		"symfony-app-dir" : "app",
		"symfony-assets-install" : "symlink",
		"incenteev-parameters" : {
			"file" : "app/config/parameters.yml"
		}
	}
}
